{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "articles",
      "pageBundle": true,
      "previewPath": "{{slug}}",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "featuredImage",
          "name": "featuredImage",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "string"
        },
        {
          "title": "url",
          "name": "url",
          "type": "slug"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "about",
      "path": "[[workspace]]/content/about"
    },
    {
      "title": "articles",
      "path": "[[workspace]]/content/articles",
      "contentTypes": [
        "article"
      ]
    }
  ],
  "frontMatter.taxonomy.series": [
    {
      "id": "customTaxonomy",
      "options": [
        "Introduction to Computers",
        "Make a Game with RPG Maker MV"
      ]
    }
  ]
}